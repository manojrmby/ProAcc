<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet"
          type="text/css"
          href="~/Asset/css/Menu.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css" />
    <script type="text/javascript" src="~/Asset/scripts/main.js"></script>
    
</head>
<body>

    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <div class="app-header header-shadow app-theme-white sidebar-text-dark">
            @*<div class="app-header header-shadow">*@
            <div class="app-header__logo ">
                <div class="logo-src"></div>
                <div class="header__pane ml-auto ">
                    <div>
                        <button type="button"
                                class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="app-header__mobile-menu ">
                <div>
                    <button type="button"
                            class="hamburger hamburger--elastic mobile-toggle-nav">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>
            <div class="app-header__menu">
                <span>
                    <button type="button"
                            class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                        <span class="btn-icon-wrapper">
                            <i class="fa fa-ellipsis-v fa-w-6"></i>
                        </span>
                    </button>
                </span>
            </div>
            <div class="app-header__content bg-happy-green header-text-light">
                <div class="app-header-left">
                    <div class="search-wrapper">
                        <div class="input-holder">
                            <input type="text"
                                   class="search-input"
                                   placeholder="Type to search" />
                            <button class="search-icon"><span></span></button>
                        </div>
                        <button class="close"></button>
                    </div>

                </div>
                <div class="app-header-right">
                    <a href="javascript:void(0);" class="nav-link">
                        <i class="nav-link-icon fa fa-envelope-square"></i>
                    </a>
                    <a href="javascript:void(0);" class="nav-link">
                        <i class="nav-link-icon fa fa-bell"></i>
                    </a>

                    <div class="header-btn-lg pr-0">
                        <div class="widget-content p-0">
                            <div class="widget-content-wrapper">
                                <div class="widget-content-left">
                                    <div class="btn-group">
                                        <a data-toggle="dropdown"
                                           aria-haspopup="true"
                                           aria-expanded="false"
                                           class="p-0 btn">
                                            <img width="42"
                                                 class="rounded-circle"
                                                 src="~/Asset/images/1.jpg"
                                                 alt="" />
                                            <i class="fa fa-angle-down ml-2 opacity-8"></i>
                                        </a>
                                        <div tabindex="-1"
                                             role="menu"
                                             aria-hidden="true"
                                             class="dropdown-menu dropdown-menu-right">
                                            <button type="button"
                                                    tabindex="0"
                                                    class="dropdown-item">
                                                User Account
                                            </button>
                                            <button type="button"
                                                    tabindex="0"
                                                    class="dropdown-item">
                                                Settings
                                            </button>
                                            <h6 tabindex="-1" class="dropdown-header">Header</h6>
                                            <button type="button"
                                                    tabindex="0"
                                                    class="dropdown-item">
                                                Actions
                                            </button>
                                            <div tabindex="-1" class="dropdown-divider"></div>
                                            <button type="button"
                                                    tabindex="0"
                                                    class="dropdown-item">
                                                Dividers
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                <div class="widget-content-left ml-3 header-user-info">
                                    <div class="widget-heading">
                                        Alina Mclourd
                                    </div>
                                    <div class="widget-subheading">
                                        VP People Manager
                                    </div>
                                </div>
                                <div class="widget-content-right header-user-info ml-3">
                                    <button type="button"
                                            class="btn-shadow p-1 btn btn-primary btn-sm show-toastr-example">
                                        <i class="fa text-white fa-calendar pr-1 pl-1"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="app-main">
            <div class="app-sidebar sidebar-shadow app-theme-white">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button"
                                    class="hamburger close-sidebar-btn hamburger--elastic"
                                    data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button"
                                class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button"
                                class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading"></li>
                            <li>
                                <a href="@Url.Action("Home","Home")" class="mm-active">
                                    <!-- <i class="metismenu-icon pe-7s-home"></i> -->
                                    <i class="metismenu-icon fa fa-home"></i>
                                    Dashboard
                                </a>
                            </li>
                            <!-- <li class="app-sidebar__heading"></li> -->
                            <li>
                                <a>
                                    <!-- <i class="metismenu-icon pe-7s-hourglass"></i> -->
                                    <i class="metismenu-icon fa fa-cogs"></i>
                                    Assessment
                                    <i class="metismenu-state-icon fa fa-caret-down caret-left"></i>
                                </a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("CreateAnalysis","Assessment")">
                                            <i class="metismenu-icon pe-7s-diamond"></i>
                                            Create Analysis
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="metismenu-icon pe-7s-car"></i>
                                            Readiness Report
                                            <!-- <i class="metismenu-state-icon pe-7s-angle-down caret-left"></i> -->
                                            <i class="metismenu-state-icon fa fa-caret-down caret-left"></i>
                                        </a>
                                        <ul>
                                            <li>
                                                <a href="components-tabs.html">
                                                    <i class="metismenu-icon"> </i> Readiness Report
                                                </a>
                                            </li>
                                            <li>
                                                <a href="components-accordions.html">
                                                    <i class="metismenu-icon"> </i>Simplification Report
                                                </a>
                                            </li>
                                            <li>
                                                <a href="components-notifications.html">
                                                    <i class="metismenu-icon"> </i>Custom code Analysis
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="metismenu-icon pe-7s-car"></i>
                                            Compatability Report
                                            <!-- <i
                                              class="metismenu-state-icon pe-7s-angle-down caret-left"
                                            ></i> -->
                                            <i class="metismenu-state-icon fa fa-caret-down caret-left"></i>
                                        </a>
                                        <ul>
                                            <li>
                                                <a href="components-tabs.html">
                                                    <i class="metismenu-icon"> </i> Add on Compatability
                                                    Check
                                                </a>
                                            </li>
                                            <li>
                                                <a href="components-accordions.html">
                                                    <i class="metismenu-icon"> </i>ABAP Test Cock Pit
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </li>
                            <!-- <li class="app-sidebar__heading">Assessment</li> -->
                            <!-- <li class="app-sidebar__heading">Widgets</li> -->
                            <li>
                                <a href="@Url.Action("PreConvertion","Home")">
                                    <i class="metismenu-icon fa fa-desktop"></i>
                                    Pre Convertion
                                </a>
                            </li>
                            <li>
                                <a href="dashboard-boxes.html">
                                    <i class="metismenu-icon fa fa-dot-circle"></i>
                                    Migration
                                </a>
                            </li>
                            <li>
                                <a href="dashboard-boxes.html">
                                    <i class="metismenu-icon fa fa-fighter-jet"></i>
                                    Post Conversion
                                </a>
                            </li>
                            <li>
                                <a href="dashboard-boxes.html">
                                    <i class="metismenu-icon fa fa-gavel"></i>
                                    Validation
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                @RenderBody()
                <footer style="text-indent: 1em;">Copyrights 2020 <a style="color:forestgreen">Promantus</a> All Rights Reserved</footer>
            </div>


            <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
        </div>
    </div>



    @*<script src="~/Scripts/jquery-3.4.1.min.js"></script>
        <script src="~/Scripts/popper-utils.js"></script>*@

    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>

</body>
</html>
