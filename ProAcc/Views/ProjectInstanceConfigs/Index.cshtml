@model IEnumerable<ProACC_DB.ProjectInstanceConfig>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/shared/_Layout.cshtml";
}

 <h2 class="card-title">PROJECT INSTANCE CONFIGURATION</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="mb-0 table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InstaceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerProjectConfig.ProjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.InstaceName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CustomerProjectConfig.ProjectName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.isActive)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
